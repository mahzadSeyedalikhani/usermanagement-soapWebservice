<?xml version="1.0" encoding="UTF-8"?>
<endpoints
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/jax-ws/ri/runtime http://java.sun.com/webservices/docs/2.0/jaxws/sun-jaxws.xsd"
        xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime" version="2.0">


    <endpoint
            implementation="com.digipay.controller.impl.UserControllerImpl"
            name="UserController"
            url-pattern="/user" />

    <endpoint
            implementation="com.digipay.controller.impl.RoleControllerImpl"
            name="RoleController"
            url-pattern="/role" />

    <endpoint
            implementation="com.digipay.controller.impl.PermissionControllerImpl"
            name="PermissionController"
            url-pattern="/permission" />

</endpoints>